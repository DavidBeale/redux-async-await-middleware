{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "extends": "airbnb-base",
    "settings": {
        "import/resolver": {
            node: { extensions: [".js", ".mjs"] }
        }
    },
    "rules": {
        "import/extensions": ["error", "always", {
            js: "never",
            mjs: "never"
        }],
        "comma-dangle": ["error", "never"],
        "no-use-before-define": ["off"],
        "no-unused-vars": ["warn"],
        "no-prototype-builtins": ["off"]
    }
}
